<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trackit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Util.UserApi"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TrackIt">
        <activity android:name=".BarChart_Activity"></activity>
        <activity android:name=".Show_data_Activity" />
        <activity android:name=".Exercises_bodyPart_Activity" />
        <activity android:name=".List_of_BodyPart_Activity" />
        <activity android:name=".Add_routine_popUp_Activity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".Select_bodyPart_Activity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>